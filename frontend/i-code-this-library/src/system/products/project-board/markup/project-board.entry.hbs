  <script type="module">
    // Preview hack
    // NOTE - This JS is not relevant to the submission, it is just to ensure that the preview thumnail looks ok.
    if (navigator.userAgent.includes("Headless")) {
      document.body.classList.add("mt-32")
    }

    const msgCopy = document.querySelector("#copy-msg");

    document.querySelector("#btn-copy-url").addEventListener("click", () => {
      copyToClipboard("https://icodethis.com/submissions/63683")
    })



    // utility function - copy to clipboard
    function copyToClipboard(str) {
      // Use the Clipboard API to copy the text
      navigator.clipboard.writeText(str)
        .then(function () {
          //console.log('Text successfully copied to clipboard');
          msgCopy.innerText = "URL copied to clipboard";
          msgCopy.classList.remove("opacity-0");
        })
        .catch(function (err) {
          //console.error('Unable to copy text to clipboard', err);
          msgCopy.classList.remove("opacity-0");
          msgCopy.classList.add("text-red-500");
          msgCopy.innerText = "It has not been possible to copy the URL";
        });

      setTimeout(() => {
        msgCopy.classList.add("opacity-0");
        msgCopy.classList.remove("text-red-500");

      }, 3000)
    }

  </script>
