-webkit-background-clip: text;
-webkit-text-fill-color: transparent; */


    /* **********UTILITY CLASSES********** */

    .asp-rat-1 {
      aspect-ratio: 1;
    }

    .transition-250 {
      transition: all 250ms ease-in-out;
    }

    .transition-300 {
      transition: all 300ms ease-in-out;
    }

    /* GRID AND FLEX UTILITY CLASSES */
    .row {
      display: flex;

    }

    .col {
      display: flex;
      flex-direction: column;
    }

    .grid {
      display: grid;
    }

    .pc-cen {
      place-content: center;
    }

    .pi-cen {
      place-items: center;
    }

    .ac-st {
      align-content: start;
    }

    .ac-cen {
      align-content: center;
    }

    .ac-end {
      align-content: end;
    }

    .ac-sb {
      align-content: space-between;
    }

    .ac-sa {
      align-content: space-around;
    }

    .ai-st {
      align-items: start;
    }

    .ai-cen {
      align-items: center;
    }

    .ai-end {
      align-items: end;
    }

    .jc-st {
      justify-content: start;
    }

    .jc-cen {
      justify-content: center;
    }

    .jc-end {
      justify-content: end;
    }

    .jc-sb {
      justify-content: space-between;
    }

    .jc-sa {
      justify-content: space-around;
    }

    .jc-se {
      justify-content: space-evenly;
    }

    .as-st {
      align-self: start;
    }

    .as-cen {
      align-self: center;
    }

    .as-end {
      align-self: end;
    }

    .js-st {
      justify-self: start;
    }

    .js-cen {
      justify-self: center;
    }

    .js-end {
      justify-self: end;
    }

    .fg-1 {
      flex-grow: 1;
    }

    .fs-0 {
      flex-shrink: 0;
    }

    .fw-wrap {
      flex-wrap: wrap;
    }

    .fb-100 {
      flex-basis: 100%;
    }

    .gap-2 {
      gap: 2em;
    }

    .gap-1 {
      gap: 1em;
    }

    .gap-half {
      gap: .5em;
    }

    .gap-quarter {
      gap: .25em;
    }

    /* ELEMENT SELECTORS */

    body {
      font-family: var(--ff-primary);
      font-size: var(--fs-body);
      font-weight: 400;
      line-height: 1.4;
      color: var(--clr-primary);
      text-wrap: pretty;
      margin: 0;
      background-color: #000;
      background-image: var(--clr-bg-body);
      display: flex;
      justify-content: center;
      align-items: center;
      min-height: 100vh;
    }

    h1 {
      font-size: var(--fs-h1);
      text-wrap: balance;
      line-height: 1;
      font-weight: 600;
    }

    h2 {
      font-size: var(--fs-h2);
      line-height: 1;
      font-weight: 600;
    }

    h3 {
      font-size: var(--fs-h3);
      line-height: 1;
      font-weight: 600;
    }

    .small {
      font-size: var(--fs-small);
    }

    .lg {
      font-size: 1.1em;
    }

    .desat {
      opacity: 0.8;
    }

    .bold {
      font-weight: 600;
    }

    /* LAYOUT */

    .card {
      background-color: var(--clr-bg-card);
      padding: 1em 1em 4em;
      border-radius: 1em;
      width: min(100%, 30em);
    }

    /* PROJECT SPECIFIC STYLES */

    .header button {
      transition: color 300ms;
    }

    .header button:is(:hover, :focus-visible) {
      color: var(--clr-accent);
    }

    .logo {
      font-family: 'Anton';
      font-size: 1.5em;
    }

    .logo-icon>* {
      color: var(--clr-accent);
      font-size: var(--fs-body);
      margin-left: 0.125em;
    }


    .nav {
      position: fixed;
      left: 50%;
      transform: translateX(-50%);
      padding: 0.3em;
      /* background-color: rgba(0, 0, 0, 0.5); */
      border-radius: 100vw;
      font-size: 1.3em;
      background-color: var(--clr-accent-alpha);
    }

    .nav ul {
      display: flex;
      gap: 0.5em;
      align-items: center;
    }

    .icon button {
      display: grid;
      place-content: center;
      background-color: rgb(255, 255, 255);
      color: var(--clr-accent);
      padding: 0.5em;
      border-radius: 50%;
      aspect-ratio: 1;
      transition: background-color 300ms, color 300ms;
      position: relative;

      &:is(:hover, :focus-visible) {
        background-color: black;
        color: white;
        box-shadow: 0 0.25em 0.5em rgb(0 0 0 / 0.2);
      }
    }

    .icon button:has(.fa-bell)::before {
      content: '2';
      position: absolute;
      font-size: var(--fs-small);
      background: var(--clr-accent);
      color: white;
      aspect-ratio: 1;
      padding: 0.25em 0.75em;
      border-radius: 50%;
      top: 0;
      right: 0;
      transform: translateY(-50%) translateX(25%);
      transition: background-color 300ms;
    }

    .icon button:has(.fa-bell):is(:hover, :focus-visible)::before {
      background-color: var(--clr-neutral-900);
    }

    .main {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(min(100%, 8em), 1fr));
      column-gap: 1em;
    }

    .profile {
      text-align: center;
      padding: 3em 0 0;
      position: relative;
      margin-block: 1em;
      display: grid;
      grid-template-columns: subgrid;
    }

    .profile-border {
      border: 2px solid var(--clr-accent);
      border-radius: 1em;
      padding: 1em 0.5em 0.5em;
    }

    .profile .lg {
      margin-block-end: 0.325rem;
    }

    .profile img {
      border-radius: 50%;
      margin-inline: auto;
      border: 2px solid var(--clr-accent);
      position: absolute;
      top: 0;
      left: 50%;
      transform: translateX(-50%);
      object-fit: cover;
    }

    .modal[open] {
      width: min(100%, 25em);
      border: none;
      padding: 1.5em;
      border-radius: 1em;
      display: grid;
      gap: 1rem;
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);

      &:is(focus) {
        outline: none;
      }
    }

    .modal[open] [btn-close] {
      position: absolute;
      top: 0;
      right: 0;
      padding: 0.5em 1em;
      border: none;

      &:is(:focus) {
        outline: none;
        scale: 1.2;
        color: var(--clr-accent);
      }

    }

    .modal::backdrop {
      background-color: rgba(0, 0, 0, 0.7);
    }

    .tag {
      /* background-color: var(--clr-accent-alpha); */
      /* color: var(--clr-neutral-100); */
      padding: 0.25em;
      border-radius: 0.25em;
      /* border: 1px solid var(--clr-accent); */
    }

    .btn {
      background-color: var(--clr-accent);
      color: var(--clr-neutral-100);
      padding: 0.5em;
      font-weight: 600;
      border-radius: 0.5em;
      transition: background-color 300ms;

      &:is(:hover, :focus-visible) {
        background-color: var(--clr-accent-alpha);
        outline: none;
      }
    }
  </style>
  <script type="module">
    const modal = document.querySelector('.modal');
    const modalBtns = modal.querySelectorAll('button');

    setTimeout(() => {
      modal.showModal();
      modalBtns.forEach((btn) => {
        btn.blur();
      })
    }, 3000);

    modalBtns.forEach((btn) => {
      btn.addEventListener('click', () => {
        modal.close();
      })
    })

  </script>
